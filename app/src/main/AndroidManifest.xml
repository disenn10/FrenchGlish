<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.disen.frenglish">

    <application
        android:allowBackup="true"
        android:icon="@drawable/iconfr"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ColorsActivity"
            android:label="@string/colors"/>
        <activity android:name=".PhrasesActivity"
            android:label="@string/phrases"/>
        <activity android:name=".FamilyMembersActivity"
            android:label="@string/family"/>
        <activity android:name=".WordsActivity"
            android:label="@string/family"/>
        <activity android:name=".NumbersActivity"
            android:label="@string/numbers">

        </activity>
    </application>

</manifest>